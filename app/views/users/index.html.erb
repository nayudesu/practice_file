<div>
      <%= render 'books/error', book: @book %>
      <%= render 'profile', user: @user %>
      <%= render 'books/newbook', book: @book %>
</div>

<h1>Books</h1>
<table>
<thead>
  <tr>
    <th>Title</th>
    <th>Opinion</th>
    <th></th>
  </tr>
</thead>
<tbody>
  <%= @user.each do |user| %>
      <tr>
      <td>
        <%= image_tag user.get_profile_image(100,100) %>
      </td>
      <td><%= user.name %></td>
      <td><%= link_to "Show", user_path(user.id) %></td>
      
      
      <% end %>
      </td>
    </tr>
  <% end %>
</tbody>

